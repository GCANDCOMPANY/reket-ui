{
  "name": "@reket/ui",
  "version": "0.2.2",
  "description": "Reket UI Library",
  "license": "MIT",
  "homepage": "https://github.com/GCANDCOMPANY/reket-ui",
  "private": false,
  "type": "module",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "design system",
    "ui library",
