{
  "name": "@reket/ui",
  "version": "0.2.10",
  "description": "Reket UI Library",
  "license": "MIT",
  "homepage": "https://github.com/GCANDCOMPANY/reket-ui",
  "private": false,
  "type": "module",
  "main": "./dist/index.umd.js",
